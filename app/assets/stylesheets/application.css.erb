.bubble 
{
position: relative;
width: auto;
min-width: 250px;
max-width: 400px;
height: auto;
padding: 10px;
background: #FFFFFF;
-webkit-border-radius: 14px;
-moz-border-radius: 14px;
border-radius: 14px;
border: rgb(240,240,240) solid 5px;
margin-bottom: 20px;
}

.bubble:after 
{
content: '';
position: absolute;
border-style: solid;
border-width: 15px 15px 0;
border-color: #FFFFFF transparent;
display: block;
width: 0;
z-index: 1;
margin-left: -15px;
bottom: -15px;
left: 12%;
}

.bubble:before 
{
content: '';
position: absolute;
border-style: solid;
border-width: 19px 19px 0;
border-color: rgb(240,240,240) transparent;
display: block;
width: 0;
z-index: 0;
margin-left: -19px;
bottom: -24px;
left: 12%;
}

